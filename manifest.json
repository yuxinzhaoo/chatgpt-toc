{
  "manifest_version": 3,
  "name": "ChatGPT 问答目录",
  "version": "1.0",
  "description": "为 ChatGPT 添加目录导航功能",
  "permissions": ["scripting", "activeTab"],
  "content_scripts": [
    {
      "matches": ["https://chatgpt.com/*"],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ],
  "icons": {
    "48": "icons/icon.png"
  }
}
